! Xft font settings
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.dpi: 96
Xft.rgba: rgb

! urxvt settings
URxvt*termName:rxvt-256color
URxvt*loginShell:true
URxvt.scrollBar: false
URxvt*scrollTtyOutput:      false
URxvt*scrollWithBuffer:     true
URxvt*scrollTtyKeypress:    true
URxvt.depth: 32
URxvt.saveLines: 50000
URxvt.urgentOnBell: true
URxvt.font:xft:Dina-11

! xterm settings
xterm*termName: xterm-256color
xterm*faceName: Dejavu Sans Mono:size=9

!Black
*.color0: #000000
*.color8: #5D5D5D

!Red
*.color1: #C75646
*.color9: #E09690

!Green
*.color2: #8EB33B
*.color10: #CDEE69

!Yellow
*.color3: #D0B03C
*.color11: #FFE377

!Blue
*.color4:  #72B3CC
*.color12: #9CD9F0

!Magenta
*.color5:  #C8A0D1
*.color13: #FBB1F9

!Cyan
*.color6:  #218693
*.color14: #77DFD8

!White
*.color7: #B0B0B0
*.color15: #F7F7F7

*.background: #1B1B1B
!*.colorBD: #FFFFFF
*.cursorColor: #218693
!*.cursorColor2: #FFFFFF
*.foreground: #F8F8F8
