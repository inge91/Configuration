! Xft font settings
! Xft.autohint: 0
! Xft.lcdfilter: lcddefault
! Xft.hintstyle: hintmedium
! Xft.hinting: 1
! Xft.antialias: 1
! Xft.dpi: 96
! Xft.rgba: rgb

! urxvt settings
URxvt*termName:rxvt-256color
URxvt*loginShell:true
URxvt.scrollBar: false
URxvt*scrollTtyOutput:      false
URxvt*scrollWithBuffer:     true
URxvt*scrollTtyKeypress:    true
URxvt.saveLines: 50000
URxvt.urgentOnBell: true
URxvt.font:xft:Terminus-12

! xterm settings
xterm*termName: xterm-256color
xterm*faceName: Terminus:size=12
xterm*scrollBar: false

URxvt.background: #000000
URxvt.foreground: #ffffff
URxvt.color0: #7C7C7C
URxvt.color1: #e18964
URxvt.color2: #a8ff60
URxvt.color3: #ffffb6
URxvt.color4: #6699cc
URxvt.color5: #ff73fd
URxvt.color6: #96cdfe
URxvt.color7: #eeeeee
URxvt.color8: #7C7C7C
URxvt.color9: #e18964
URxvt.color10: #a8ff60
URxvt.color11: #ffffb6
URxvt.color12: #6699cc
