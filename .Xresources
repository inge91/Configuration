! Xft font settings
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintmedium
Xft.hinting: 1
Xft.antialias: 1
Xft.dpi: 96
Xft.rgba: rgb

! urxvt settings
URxvt*termName:rxvt-256color
URxvt*loginShell:true
URxvt.scrollBar: false
URxvt*scrollTtyOutput:      false
URxvt*scrollWithBuffer:     true
URxvt*scrollTtyKeypress:    true
URxvt.depth: 32
URxvt.saveLines: 50000
URxvt.urgentOnBell: true
URxvt.font:xft:Cousine-9

! xterm settings
xterm*termName: xterm-256color
xterm*faceName: Cousine:size=12pt

! Colors.
xterm*background: #0D131A
xterm*foreground: #8A9499
URxvt*background: #0D131A
URxvt*foreground: #8A9499

! black
*color0: #0D131A
*color8: #44484C

! red
*color1: #802635
*color9: #B3354C

! green
*color2: #608040
*color10: #4BA65A

! yellow
*color3: #877C43
*color11: #BF9F5F

! blue
*color4: #1C678C
*color12: #3995BF

! magenta
*color5: #684D80
*color13: #A64286

! cyan
*color6: #337373
*color14: #6AAAB3

! white
*color7: #738080
*color15: #B3B3B3
