! Xft font settings
! Xft.autohint: 0
! Xft.lcdfilter: lcddefault
! Xft.hintstyle: hintmedium
! Xft.hinting: 1
! Xft.antialias: 1
! Xft.dpi: 96
! Xft.rgba: rgb

! urxvt settings
URxvt*termName:rxvt-256color
URxvt*loginShell:true
URxvt.scrollBar: false
URxvt*scrollTtyOutput:      false
URxvt*scrollWithBuffer:     true
URxvt*scrollTtyKeypress:    true
URxvt.saveLines: 50000
URxvt.urgentOnBell: true
URxvt.font:xft:Terminus-12

! xterm settings
xterm*termName: xterm-256color
xterm*faceName: Terminus:size=12
xterm*scrollBar: false

! Jellybeans Terminal Colours
! vim: ft=xdefaults
urxvt*background: #151515
urxvt*foreground: #888888
! black
*color0: #1C1C1C
*color8: #404040
! red
*color1: #B85335
*color9: #CF6A4C
! green
*color2: #799D6A
*color10: #99AD6A
! yellow
*color3: #FFB964
*color11: #FAD07A
! blue
*color4: #667899
*color12: #8197BF
! magenta
*color5: #8787AF
*color13: #C6B6EE
! cyan
*color6: #668799
*color14: #8FBFDC
! white
*color7: #888888
*color15: #E8E8D3
