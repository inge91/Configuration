Config { font = "xft:Cousine-9"
       , bgColor = "#151515"
       , fgColor = "#D7D0C7"
       , position = TopP 0 100
       , commands = [ Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
                    , Run Memory ["-t","Mem: <usedratio>%"] 10
                    , Run Swap [] 10
                    , Run Com "uname" ["-s","-r"] "" 36000
                    , Run Date "%a %b %_d %Y %H:%M" "date" 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ %cpu%        %memory% * %swap%        <fc=#A0CF5D>%date%</fc>"
       }
