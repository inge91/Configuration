Config { font = "xft:Inconsolata-10.5"
       , bgColor = "#151515"
       , fgColor = "#D7D0C7"
       , position = TopP 0 100
       , commands = [ Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
                    , Run Date "%a %b %_d %Y %H:%M" "date" 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ %cpu%    %date%"
       }
